{
  "manifest_version": 2,
  "name": "Detrollify",
  "version": "1.6",

  "applications": {
    "gecko": {
      "id": "detrollify@jeffwax.com"
    }
  },

  "description": "Translate the translatable Troll Pesterchum quirks from Homestuck.",

  "icons": {
    "48": "icons/detrollify-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.homestuck.com/*"],
      "js": ["assets/js/jquery-3.3.1.min.js", "detrollify.js"]
    }
  ]

}
